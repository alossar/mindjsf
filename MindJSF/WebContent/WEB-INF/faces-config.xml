<?xml version="1.0" encoding="UTF-8"?>

<faces-config xmlns="http://java.sun.com/xml/ns/javaee"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-facesconfig_2_1.xsd"
	version="2.1">
	<managed-bean>
		<managed-bean-name>loginController</managed-bean-name>
		<managed-bean-class>co.mind.web.login.LoginController</managed-bean-class>
		<managed-bean-scope>request</managed-bean-scope>
		<managed-property>
			<property-name>correo</property-name>
			<property-class>java.lang.String</property-class>
			<value></value>
		</managed-property>
		<managed-property>
			<property-name>pass</property-name>
			<property-class>java.lang.String</property-class>
			<value></value>
		</managed-property>
		<managed-property>
			<property-name>correoRecuperar</property-name>
			<property-class>java.lang.String</property-class>
			<value></value>
		</managed-property>
	</managed-bean>
	<managed-bean>
		<managed-bean-name>indexController</managed-bean-name>
		<managed-bean-class>co.mind.web.index.IndexController</managed-bean-class>
		<managed-bean-scope>request</managed-bean-scope>
	</managed-bean>
	<managed-bean>
		<managed-bean-name>homeController</managed-bean-name>
		<managed-bean-class>co.mind.web.index.HomeController</managed-bean-class>
		<managed-bean-scope>request</managed-bean-scope>
	</managed-bean>
	<managed-bean>
		<managed-bean-name>evaluadoEspecificoController</managed-bean-name>
		<managed-bean-class>co.mind.web.evaluados.EvaluadoEspecificoController</managed-bean-class>
		<managed-bean-scope>request</managed-bean-scope>
	</managed-bean>
	<managed-bean>
		<managed-bean-name>programadorEspecificoController</managed-bean-name>
		<managed-bean-class>co.mind.web.programadores.ProgramadorEspecificoController</managed-bean-class>
		<managed-bean-scope>request</managed-bean-scope>
	</managed-bean>
	<managed-bean>
		<managed-bean-name>autorizacionController</managed-bean-name>
		<managed-bean-class>co.mind.web.autorizacion.AutorizacionController</managed-bean-class>
		<managed-bean-scope>request</managed-bean-scope>
	</managed-bean>
	<navigation-rule>
		<display-name>login</display-name>
		<from-view-id>/login.jsp</from-view-id>
		<navigation-case>
			<from-outcome>recuperar</from-outcome>
			<to-view-id>/recuperar.jsp</to-view-id>
		</navigation-case>
	</navigation-rule>
	<navigation-rule>
		<display-name>login</display-name>
		<from-view-id>/login.jsp</from-view-id>
		<navigation-case>
			<from-outcome>index</from-outcome>
			<to-view-id>/index.xhtml</to-view-id>
		</navigation-case>
	</navigation-rule>
	<navigation-rule>
		<display-name>index</display-name>
		<from-view-id>/index.xhtml</from-view-id>
		<navigation-case>
			<from-outcome>procesos</from-outcome>
			<to-view-id>/procesos.xhtml</to-view-id>
		</navigation-case>
	</navigation-rule>
	<navigation-rule>
		<display-name>procesos</display-name>
		<from-view-id>/procesos.xhtml</from-view-id>
		<navigation-case>
			<from-outcome>proceso</from-outcome>
			<to-view-id>/proceso.xhtml</to-view-id>
		</navigation-case>
	</navigation-rule>
	<navigation-rule>
		<display-name>index</display-name>
		<from-view-id>/index.xhtml</from-view-id>
		<navigation-case>
			<from-outcome>pruebas</from-outcome>
			<to-view-id>/pruebas.xhtml</to-view-id>
		</navigation-case>
	</navigation-rule>
	<navigation-rule>
		<display-name>index</display-name>
		<from-view-id>/index.xhtml</from-view-id>
		<navigation-case>
			<from-outcome>evaluados</from-outcome>
			<to-view-id>/evaluados.xhtml</to-view-id>
		</navigation-case>
	</navigation-rule>
	<navigation-rule>
		<display-name>index</display-name>
		<from-view-id>/index.xhtml</from-view-id>
		<navigation-case>
			<from-outcome>clientes</from-outcome>
			<to-view-id>/clientes.xhtml</to-view-id>
		</navigation-case>
	</navigation-rule>
	<navigation-rule>
		<display-name>index</display-name>
		<from-view-id>/index.xhtml</from-view-id>
		<navigation-case>
			<from-outcome>programadores</from-outcome>
			<to-view-id>/programadores.xhtml</to-view-id>
		</navigation-case>
	</navigation-rule>
	<navigation-rule>
		<display-name>pruebas</display-name>
		<from-view-id>/pruebas.xhtml</from-view-id>
		<navigation-case>
			<from-outcome>prueba</from-outcome>
			<to-view-id>/prueba.xhtml</to-view-id>
		</navigation-case>
	</navigation-rule>
	<navigation-rule>
		<display-name>evaluados</display-name>
		<from-view-id>/evaluados.xhtml</from-view-id>
		<navigation-case>
			<from-outcome>evaluado</from-outcome>
			<to-view-id>/evaluado.xhtml</to-view-id>
		</navigation-case>
	</navigation-rule>
	<navigation-rule>
		<display-name>index</display-name>
		<from-view-id>/index.xhtml</from-view-id>
		<navigation-case>
			<from-outcome>cuenta</from-outcome>
			<to-view-id>/cuenta.xhtml</to-view-id>
		</navigation-case>
	</navigation-rule>
	<navigation-rule>
		<display-name>clientes</display-name>
		<from-view-id>/clientes.xhtml</from-view-id>
		<navigation-case>
			<from-outcome>cliente</from-outcome>
			<to-view-id>/cliente.xhtml</to-view-id>
		</navigation-case>
	</navigation-rule>
	<navigation-rule>
		<display-name>programadores</display-name>
		<from-view-id>/programadores.xhtml</from-view-id>
		<navigation-case>
			<from-outcome>programador</from-outcome>
			<to-view-id>/programador.xhtml</to-view-id>
		</navigation-case>
	</navigation-rule>
</faces-config>
